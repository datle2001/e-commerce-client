{
  "name": "e-commerce-client",
  "version": "0.0.0",
  "homepage": "https://datle2001.github.io/e-commerce-client/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "watch": "ng build --watch --configuration development",
    "build": "ng build && npm run copy-index-to-404 && npm run commit-push",
    "copy-index-to-404": "npm install -g shx && cd docs && dir && shx cp index.html 404.html",
    "commit-push": "git add -A && git commit -F - && git push"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.1.1",
    "@angular/common": "^15.1.0",
